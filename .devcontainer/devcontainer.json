{
    "name": "Node.js",
    "build": {
      "dockerfile": "Dockerfile",
      "context": ".."
    },
    "workspaceFolder": "/workspace",
    "mounts": [
      "source=${localWorkspaceFolder},target=/workspace,type=bind",
      "source=node_modules,target=/workspace/node_modules,type=volume"
    ],
  }